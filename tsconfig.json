{
  "compilerOptions": {
    "module": "commonjs",
    // "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "jsx": "react",
    "resolveJsonModule": true,
    "incremental": true,
    "skipLibCheck": true
  }
}